{
    "name": "cgtag/php-exceptions",
    "description": "A collection of common throwable exceptions",
    "keywords": ["exceptions","php","library"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "cgTag",
            "homepage": "https://cgTag.com/"
        }
    ],
    "support": {
        "issues": "https://github.com/cgTag/php-exceptions/issues",
        "source": "https://github.com/cgTag/php-exceptions"
    },
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.7,<6"
    },
    "autoload": {
        "psr-4": {
            "cgTag\\Exceptions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "cgTag\\Exceptions\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --no-coverage"
    }
}
